{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/user/**",
        "function": "ogp"
      },
      {
        "source": "/image/**",
        "function": "ogp"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
